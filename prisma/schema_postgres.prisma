generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://surya:nemi@localhost:5432/nemi"
}

model NemiTables {
  id        Int      @id @default(autoincrement())
  tableName String   @unique
  columns   Json
  createdAt DateTime @default(now())
}
